{
  "name": "@fenixengine/wizard",
  "description": "FeniXEngine Wizard is a tool for all your RPG Maker plugin projects",
  "version": "3.1.0",
  "author": "FeniXEngine Contributors",
  "type": "module",
  "main": "bin/fenix-wizard.js",
  "exports": {
    ".": {
      "require": "./bin/fenix-wizard.cjs",
      "import": "./bin/fenix-wizard.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/FeniXEngineMV/wizard"
  },
  "license": "MIT",
  "bin": {
    "fenix": "./bin/fenix-wizard.js"
  },
  "files": [
    "/bin/",
    "/templates/",
    "/README.md",
    "docs/.vuepress/public/img/fenixwizard-init.gif",
    "docs/.vuepress/public/img/logos/fenixwizard-text-logo.png"
  ],
  "scripts": {
    "build": "rollup -c",
    "docs:dev": "eleventy --serve",
    "docs:build": "cross-env NODE_ENV=production eleventy",
    "test": "ava --verbose",
    "lint": "eslint ./src/**"
  },
  "dependencies": {
    "@fenixengine/rollup-plugin-jscc": "^2.2.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cli-cursor": "^4.0.0",
    "commander": "^11.1.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^11.2.0",
    "nanospinner": "^1.1.0",
    "pretty-error": "^4.0.0",
    "prompts": "^2.4.2",
    "rollup": "^4.6.1",
    "time-stamp": "^2.2.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "ava": "^5.3.1",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "eol": "^0.9.1",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "markdown-it-anchor": "^8.6.7",
    "node-fetch": "^2.6.1",
    "rollup-plugin-eslint-bundle": "^5.0.2",
    "tempy": "^0.2.1",
    "test-console": "^2.0.0",
    "upath": "^2.0.1"
  },
  "ava": {
    "files": [
      "!./test/helpers/output/",
      "!./test/fixtures/**"
    ],
    "require": [
      "./test/_globals.js"
    ]
  }
}
